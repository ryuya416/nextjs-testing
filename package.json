    "setimmediate": "^1.0.5",
    "swr": "^1.3.0",
    "tailwindcss": "^3.1.3"
  },
  "devDependencies": {
    "@testing-library/dom": "7.29.2",
    "@testing-library/jest-dom": "5.11.8",
    "@testing-library/react": "11.2.3",
    "@testing-library/user-event": "12.6.0",
    "@types/jest": "26.0.20",
    "@types/node": "^18.0.0",
    "@types/react": "17.0.41",
    "babel-jest": "26.6.3",
    "eslint": "8.17.0",
